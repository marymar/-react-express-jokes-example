    "react-dom": "^16.1.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.1.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  }
